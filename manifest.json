{
  "name": "Dark Settings",
  "version": "1.0",
  "description": "Darken chrome settings page",
  "manifest_version": 2,
  "permissions": ["declarativeContent", "storage", "activeTab", "chrome://*/"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
